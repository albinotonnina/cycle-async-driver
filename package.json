{
  "name": "cycle-async-driver",
  "version": "1.4.0",
  "description": "Helper for creating async request/response cycle.js drivers",
  "keywords": ["cyclejs", "driver", "async"],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && babel src -d lib",
    "build-watch": "babel -w src -d lib",
    "test": "node -r babel-register test/test.js",
    "test-watch": "node-dev --respawn -r babel-register test/test.js",
    "vmd": "vmd README.md",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "whitecolor",
  "license": "ISC",
  "devDependencies": {
    "@cycle/core": "^6.0.0",
    "@cycle/isolate": "^1.2.0",
    "rx": "^4.0.7",
    "tape": "^4.4.0",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
